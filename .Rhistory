#' <Add Description>
#'
#' @import htmlwidgets
#'
#' @export
LACEview <- function(message, width = NULL, height = NULL, elementId = NULL) {
# forward options using x
x = list(
message = message
)
# create widget
htmlwidgets::createWidget(
name = 'LACEview',
x,
width = width,
height = height,
package = 'LACEview',
elementId = elementId
)
}
